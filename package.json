{
    "name": "airtheon",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "npm run format && next dev --turbopack",
        "build": "npm run format && next build",
        "start": "next start",
        "lint": "next lint",
        "format": "count=$(prettier --write . --log-level silent | grep -v '(unchanged)' | wc -l); echo \"$count files formatted\"",
        "check": "npm run lint && npm run format:check",
        "format:check": "prettier --check .",
        "email": "email dev --dir src/utils/emails",
        "pages:build": "bunx @cloudflare/next-on-pages",
        "preview": "bun pages:build && wrangler pages dev --experimental-vectorize-bind-to-prod --ai",
        "deploy": "bun pages:build && wrangler pages deploy",
        "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
        "generate": "drizzle-kit generate --config ./drizzle.config.ts",
        "push": "drizzle-kit push --config ./drizzle.config.ts",
        "studio": "drizzle-kit studio --config ./drizzle.config.ts"
    },
    "dependencies": {
        "@arcjet/next": "^1.0.0-beta.1",
        "@aws-sdk/client-s3": "^3.735.0",
        "@hono/zod-validator": "^0.4.2",
        "@hookform/resolvers": "^3.10.0",
        "@radix-ui/react-avatar": "^1.1.2",
        "@radix-ui/react-checkbox": "^1.1.3",
        "@radix-ui/react-collapsible": "^1.1.2",
        "@radix-ui/react-dialog": "^1.1.5",
        "@radix-ui/react-dropdown-menu": "^2.1.5",
        "@radix-ui/react-label": "^2.1.1",
        "@radix-ui/react-navigation-menu": "^1.2.4",
        "@radix-ui/react-popover": "^1.1.5",
        "@radix-ui/react-progress": "^1.1.1",
        "@radix-ui/react-radio-group": "^1.2.2",
        "@radix-ui/react-scroll-area": "^1.2.2",
        "@radix-ui/react-select": "^2.1.5",
        "@radix-ui/react-separator": "^1.1.1",
        "@radix-ui/react-slot": "^1.1.1",
        "@radix-ui/react-switch": "^1.1.2",
        "@radix-ui/react-tabs": "^1.1.2",
        "@radix-ui/react-tooltip": "^1.1.7",
        "@react-email/components": "0.0.31",
        "@tanstack/react-query": "^5.64.2",
        "better-auth": "^1.1.14",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cmdk": "^1.0.4",
        "date-fns": "^4.1.0",
        "drizzle-orm": "^0.38.4",
        "drizzle-zod": "^0.5.1",
        "emoji-picker-react": "^4.12.0",
        "hono": "^4.6.18",
        "input-otp": "^1.4.2",
        "lodash": "^4.17.21",
        "lucide-react": "^0.468.0",
        "motion": "^11.18.2",
        "next": "15.1.0",
        "next-themes": "^0.4.4",
        "prettier-plugin-classnames": "^0.7.5",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-hook-form": "^7.54.2",
        "react-icons": "^5.4.0",
        "resend": "^4.1.1",
        "sonner": "^1.7.2",
        "tailwind-merge": "^2.6.0",
        "tailwindcss-animate": "^1.0.7",
        "usehooks-ts": "^3.1.0",
        "wrangler": "^3.105.1",
        "zod": "^3.24.1"
