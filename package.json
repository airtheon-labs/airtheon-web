        "@types/react": "19.0.1",
        "@types/react-dom": "19.0.3",
        "drizzle-kit": "^0.30.2",
        "postcss": "^8.5.1",
        "react-email": "3.0.4",
        "tailwindcss": "^3.4.17",
        "typescript": "^5.7.3",
        "vercel": "^39.3.0"
    }
}
